{
  "name": "univ-fs-gcs",
  "version": "0.8.0",
  "description": "The universal Filesystem API for Google Cloud Storage",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "fs",
    "filesystem",
    "storage",
    "isomorphic",
    "universal",
    "google"
  ],
  "scripts": {
    "clean": "rimraf ./lib ./tsconfig.tsbuildinfo",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "npm-version": "npm version patch",
    "npm-publish": "npm publish",
    "release": "npm-run-all build npm-version npm-publish",
    "jest": "jest --runInBand",
    "test": "npm-run-all compile jest"
  },
  "jest": {
    "testTimeout": 10000,
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "<rootDir>/lib/__tests__/**/*.spec.js"
    ]
  },
  "devDependencies": {
    "@google-cloud/storage": "^6.5.0",
    "univ-fs": "file:../../core"
  },
  "peerDependencies": {
    "@google-cloud/storage": "^6.5.0",
    "univ-fs": "file:../../core"
  },
  "author": "Yoshihiro Seki",
  "license": "MIT"
}
