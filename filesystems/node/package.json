{
  "name": "univ-fs-node",
  "version": "0.9.1",
  "description": "The universal Filesystem for Node",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "fs",
    "filesystem",
    "storage",
    "isomorphic",
    "universal",
    "node"
  ],
  "scripts": {
    "clean": "rimraf ./lib ./tsconfig.tsbuildinfo",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "npm-version": "npm version patch",
    "npm-publish-dry-run": "npm publish --dry-run",
    "npm-publish": "npm publish",
    "jest": "jest --runInBand",
    "test": "npm-run-all compile jest",
    "sanitize": "rimraf ./lib/**/__tests__/",
    "release-dry-run": "npm-run-all build sanitize npm-publish-dry-run",
    "release": "npm-run-all build sanitize npm-version npm-publish"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "<rootDir>/lib/__tests__/**/*.spec.js"
    ]
  },
  "devDependencies": {
    "univ-fs": "file:../../core"
  },
  "peerDependencies": {
    "univ-conv": "~0.9.0"
  },
  "author": "Yoshihiro Seki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikochan2k/universal-fs.git"
  },
  "bugs": {
    "url": "https://github.com/nikochan2k/universal-fs/issues"
  },
  "homepage": "https://github.com/nikochan2k/universal-fs#readme"
}
